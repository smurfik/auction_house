<!DOCTYPE html>
<html>
<head>
  <title>AuctionHouse</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="header">
    <%= link_to image_tag("logo.jpg", class: "logo"), root_path %>
    <div class="nav">
      <div class="left"><%= link_to "Bid", "#" %></div>
      <div class="right"><%= link_to "Sign Up", "#" %></div>
      <div class="right"><%= link_to "Sign In", "#" %></div>
    </div>
    <div class="search">
      <%= form_tag "#" do %>
        <%= text_field_tag :search, nil, size: 65; %>
        <%= submit_tag "Search" %>
      <% end %>
    </div>
  </div>

<%= yield %>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCTc8wb5zAQilJ4EhAYSndwdxbrwzGr8eY" type="text/javascript"></script>

  <div id="footer">
    <div class="center">
      <%= image_tag("copyright.png") %>
      <span>Auction House</span>
    </div>
  </div>
</body>
</html>
